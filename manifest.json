{
  "manifest_version": 2,
  "name": "Mentor Statistics OpenClassrooms - English Version",
  "version": "1.2.0",
  "description": "An extension that help you to have more statistics on your incomes when you are an OpenClassrooms Mentor.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": [
        "https://openclassrooms.com/en/mentorship/dashboard/mentorship-sessions-history"
      ],
      "js": [
        "libs/jquery-3.3.1.js",
        "libs/jquery.tabletojson.js",
        "libs/format.js",
        "src/DateUtils.js",
        "config/config.js",
        "src/SessionModel.js",
        "src/Price.js",
        "src/Statistics.js",
        "src/Display.js",
        "src/Parser.js",
        "src/main.js"
      ]
    }
  ]
}
